#include <stdio.h>
int main()
{
    int soma, num1 = 0, num2 = 0;
    
    printf("Digite um número:\n");
    scanf("%d", &num1);
    
    printf("Digite um número:\n");
    scanf("%d", &num2);
    
    soma = num1 + num2;
    
    printf("A soma dos dois números digitados é: %d \n", soma);
    
    return 0;
}
